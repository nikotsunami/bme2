<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:n="urn:import:medizin.client.a_nonroo.app.client"
	xmlns:b="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
    .panelHTML {
       border: 0px solid #ddf;
       height: 100%;
       width: 100%;
     }
      .controls {
    position: absolute;
    left:0;
    right:0;
    top:3px;
    height: 2em;
    margin-left: 15px;
  }
 
  .controls table {
    position:absolute;
    right:0;
    top:0;
  }
  table button {
    background: none;
  }
  .controls button {
    display:inline;
  }
  .listView {
    position: relative;
    overflow: scroll;
  }
    .fullWidth {
    width: 100%;
  }
  .listView &gt; table {
    
  }
  .listView &gt; table td {
	overflow: scroll;
    white-space: nowrap;
    cursor: pointer;
  }
  
  
		.button {
	
		float: right;
		background: none;

		cursor: pointer;
		}

		.header {
			float: left;
		padding: 3px;
		color: #4B4A4A;
		text-shadow: #ddf 1px 1px 0;
	
		background-color: #E6EDED;
		font-size: 1.1em;
		font-weight: bold;
		widht: 100%;


		}

		.headerLine {
	
		background-color: #E6EDED;
		height: 25px;


		}

		.bar {
		padding: 3px;

		float: right;

		background-color: #E6EDED;

		}
		.details {
		padding: 3px;
		border: 1px solid #E6EDED;
		width: 100%;

		}
	.content {

		padding: 5px;
		

		}
	</ui:style>
	<g:HTMLPanel>
<g:VerticalPanel ui:field="questionPanel" spacing="15" styleName="{style.fullWidth}">
<g:HTMLPanel>Keine Antworten zu akzeptieren</g:HTMLPanel>
</g:VerticalPanel>
</g:HTMLPanel>
</ui:UiBinder> 